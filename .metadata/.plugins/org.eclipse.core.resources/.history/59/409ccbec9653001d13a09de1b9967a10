#include <stdio.h>
#include <stdlib.h>

int contarCaracteres(char cadena[],char caracter);

int main(void) {

	int retorno;

	retorno = contarCaracteres("allende", 'e');

	printf("\nSe repite : %d de veces", retorno);

	return EXIT_SUCCESS;
}

int contarCaracteres(char cadena[],char caracter)
{
	int retorno;
	int i;
	int cont=0;

	for(i = 0;i<len(cadena);i++)
	{
		if(cadena[i]==caracter)
		{
			cont++;
		}
	}
	return retorno;
}
